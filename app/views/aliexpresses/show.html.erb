<%- model_class = Aliexpress -%>
<div class="page-header">
  <h1><%= @aliexpress.name %></h1>
</div>

<dl class="dl-horizontal">
  <dt><strong><%= model_class.human_attribute_name(:email) %>:</strong></dt>
  <dd><%= @aliexpress.email %></dd>
  <dt><strong><%= model_class.human_attribute_name(:password) %>:</strong></dt>
  <dd><%= @aliexpress.password %></dd>
</dl>

<%= link_to 'Voltar',
              aliexpresses_path, :class => 'btn btn-default'  %>
<%= link_to 'Editar',
              edit_aliexpress_path(@aliexpress), :class => 'btn btn-default' %>
<%= link_to 'Deletar',
              aliexpress_path(@aliexpress),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
